{
  "expo": {
    "name": "FitTracker",
    "slug": "fittracker",
    "version": "1.1.6",
    "orientation": "portrait",
    "icon": "./assets/fittracker-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mkbuildp.fittracker",
      "buildNumber": "16"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/fittracker-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mkbuildp.fittracker",
      "versionCode": 16,
      "targetSdkVersion": 35,
      "compileSdkVersion": 35,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "blockedPermissions": [
        "com.google.android.gms.permission.AD_ID"
      ],
      "allowBackup": false,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.mkbuildp.fittracker"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-localization",
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "parentTheme": "Default",
            "enforceNavigationBarContrast": false
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "52c10f8f-e287-4109-9f3f-2f6f90a16c0a"
      }
    }
  }
}
